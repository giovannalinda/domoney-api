{
  "name": "api",
  "version": "0.0.1",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon --exec sucrase-node src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:ci": "jest --passWithNoTests --ci --watchAll=false",
    "prettier:write": "prettier --write 'src/**/**.{js,jsx,ts,tsx,json,css,less,md}'",
    "prettier:check": "prettier -c  'src/**/**.{js,jsx,ts,tsx,json,css,less,md}'",
    "lint": "eslint 'src/**/**.{js,jsx,ts,tsx}'"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.17.2",
    "pg": "8.7.1",
    "typeorm": "0.2.41"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/module-alias": "2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "dotenv": "10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "4",
    "jest": "27.4.7",
    "lint-staged": "12.1.7",
    "module-alias": "2.2.2",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "sucrase": "3.20.3",
    "ts-jest": "27.1.2",
    "typescript": "4.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/**.{js,jsx,ts,tsx}": [
      "eslint"
    ],
    "src/**/**.{js,jsx,ts,tsx,json,css,less,md}": [
      "prettier --write"
    ]
  }
}
